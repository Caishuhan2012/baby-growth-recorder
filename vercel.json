{
  "version": 2,
  "public": true,
  "name": "baby-growth-recorder",
  "alias": ["baby-growth-recorder.vercel.app"],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/wechat",
      "dest": "/api/wechat.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
} 