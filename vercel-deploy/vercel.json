{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ],
  "functions": {
    "api/wechat-handler.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    },
    "api/wechat.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 10
    }
  }
} 